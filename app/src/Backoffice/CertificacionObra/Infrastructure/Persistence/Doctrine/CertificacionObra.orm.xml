<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Backoffice\CertificacionObra\Domain\CertificacionObra" table="certificacion_obra">

        <id name="id"                               type="string"                       column="id" length="36"/>
        <field name="nombres"                       type="string"   nullable="true"     column="nombres" length="100" />
        <field name="apellido"                      type="string"   nullable="true"     column="apellido" length="100" />
        <field name="numeroObra"                    type="integer"  nullable="true"     column="numero_obra" />
        <field name="etapa"                         type="string"   nullable="true"     column="etapa" length="100" />
        <field name="permiteModificarComputo"       type="boolean"  nullable="true"     column="permite_modificar_computo" />
        <field name="certificacionManual"           type="boolean"  nullable="true"     column="certificacion_manual" />
        <field name="insGrabaCert"                  type="boolean"  nullable="true"     column="ins_graba_cert" />
        <field name="programa"                      type="string"   nullable="true"     column="programa" length="100" />
        <field name="departamento"                  type="string"   nullable="true"     column="departamento" length="100" />
        <field name="localidad"                     type="string"   nullable="true"     column="localidad" />
        <field name="codigoPostal"                  type="integer"  nullable="true"     column="codigo_postal" />
        <field name="numeroLicitacion"              type="string"   nullable="true"     column="numero_licitacion" length="100" />
        <field name="tipoLicitacion"                type="string"   nullable="true"     column="tipo_licitacion" length="100" />
        <field name="fechaLicitacion"               type="date"     nullable="true"     column="fecha_licitacion" />
        <field name="fechaInicioObra"               type="date"     nullable="true"     column="fecha_inicio_obra" />
        <field name="plazo"                         type="integer"  nullable="true"     column="plazo" />
        <field name="contratista"                   type="string"   nullable="true"     column="contratista" length="100" />
        <field name="anticipoFinancieroNacion"      type="decimal"  nullable="true"     column="anticipo_financiero_nacion" scale="12" precision="2" />
        <field name="anticipoFinancieroProvincia"   type="decimal"  nullable="true"     column="anticipo_financiero_provincia" scale="12" precision="2" />
        <field name="aporteNacion"                  type="decimal"  nullable="true"     column="aporte_nacion" scale="12" precision="2" />
        <field name="aporteProvincia"               type="decimal"  nullable="true"     column="aporte_provincia" scale="12" precision="2"/>
        <field name="ampliacionMontoNacion"         type="decimal"  nullable="true"     column="ampliacion_monto_nacion" scale="12" precision="2" />
        <field name="ampliacionMontoProvincia"      type="decimal"  nullable="true"     column="ampliacion_monto_provincia" scale="12" precision="2" />
        <field name="porcentajeEntregaNacion"       type="decimal"  nullable="true"     column="porcentaje_entrega_nacion" scale="12" precision="2" />
        <field name="porcentajeEntregaProvincia"    type="decimal"  nullable="true"     column="porcentaje_entrega_provincia" scale="12" precision="2" />
        <field name="coeficienteActivo"             type="decimal"  nullable="true"     column="coeficiente_activo" scale="12" precision="2" />
        <field name="porcentajeReparo"              type="decimal"  nullable="true"     column="porcentaje_reparo" scale="12" precision="2" />
        <field name="bapin"                         type="string"   nullable="true"     column="bapin" length="100" />
        <field name="montoContratado"               type="decimal"  nullable="true"     column="monto_contratado" scale="12" precision="2"/>
        <field name="presupuestoOficial"            type="decimal"  nullable="true"     column="presupuesto_oficial" scale="12" precision="2"/>
        <field name="costoObra"                     type="decimal"   nullable="true"    column="costo_obra" scale="12" precision="2"/>
        <field name="createAt"                      type="datetime" nullable="false"    column="create_at" />
    </entity>
</doctrine-mapping>
